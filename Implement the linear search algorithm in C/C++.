#include <iostream>
using namespace std;

int main() {
    int arr[] = {5, 3, 8, 6, 2};
    int n = 5, key = 6, found = -1;

    for(int i = 0; i < n; i++) {
        if(arr[i] == key) {
            found = i;
            break;
        }
    }

    if(found != -1)
        cout << key << " found at index " << found << endl;
    else
        cout << key << " not found" << endl;

    return 0;
}