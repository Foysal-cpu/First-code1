#include <stdio.h>

int main() {
    int arr[100], n, i, pos;

    scanf("%d", &n);
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    pos = 0;
    for (i = pos; i < n - 1; i++)
        arr[i] = arr[i + 1];
    n--;

    pos = n / 2;
    for (i = pos; i < n - 1; i++)
        arr[i] = arr[i + 1];
    n--;

    n--;

    for (i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}